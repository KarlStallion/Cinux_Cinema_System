{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"ACourse\",\n  data() {\n    return {\n      course: [],\n      studentsnumbers: '',\n      groupsnumbers: '',\n      description: ''\n    };\n  },\n  methods: {\n    fetchRecords() {\n      const id = this.$route.params.id;\n      fetch(`http://localhost:3000/api/courses/${id}`).then(response => response.json()).then(data => {\n        this.course = data;\n        console.log(this.course);\n      }).catch(err => console.log(err.message));\n    },\n    setDefaultValues() {\n      if (this.studentsnumbers) {\n        this.studentsnumbers = this.course.studentsnumbers;\n      }\n      if (this.groupsnumbers) {\n        this.groupsnumbers = this.course.groupsnumbers;\n      }\n      if (this.description) {\n        this.description = this.course.description;\n      }\n    },\n    updateCourse() {\n      this.setDefaultValues();\n      const id = this.$route.params.id;\n      console.log(this.studentsnumbers, this.groupsnumbers, this.description);\n      fetch(`http://localhost:3000/api/courses/${id}`, {\n        method: \"PUT\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          studentsnumbers: this.studentsnumbers,\n          groupsnumbers: this.groupsnumbers,\n          description: this.description\n        })\n      }).then(response => response.json()).then(data => {\n        console.log(data);\n        this.$router.push({\n          name: \"Courses\"\n        });\n      }).catch(err => console.log(err.message));\n    }\n  },\n  mounted() {\n    this.fetchRecords();\n  }\n};","map":{"version":3,"names":["name","data","course","studentsnumbers","groupsnumbers","description","methods","fetchRecords","id","$route","params","fetch","then","response","json","console","log","catch","err","message","setDefaultValues","updateCourse","method","headers","body","JSON","stringify","$router","push","mounted"],"sources":["/Users/karlparloja/Documents/WAD23-KarlMattiasParloja/src/views/ACourse.vue"],"sourcesContent":["<template>\n  <div class=\"A Course\">\n    <div class=\"form\" v-for=\"course in course\" :key=\"course.id\">\n      <h3>{{ course.coursename }} - {{ course.coursecode }} - {{ course.courseects }} ECTS</h3>\n      <label>Max Number Of Students: </label>\n      <input type=\"text\" :placeholder=\"course.studentsnumbers\" v-model=\"studentsnumbers\"/>\n      <label>Number of Groups: </label>\n      <input type=\"text\" :placeholder=\"course.groupsnumbers\" v-model=\"groupsnumbers\"/>\n      <label>Course Decription: </label>\n      <input type=\"text\" :placeholder=\"course.description\" v-model=\"description\"/>\n      <button @click=\"updateCourse\">Update Course</button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ACourse\",\n  data() {\n    return {\n      course: [],\n      studentsnumbers: '',\n      groupsnumbers: '',\n      description: '',\n    };\n  },\n  methods: {\n    fetchRecords() {\n      const id = this.$route.params.id;\n      fetch(`http://localhost:3000/api/courses/${id}`)\n          .then((response) => response.json())\n          .then((data) => {\n            this.course = data;\n            console.log(this.course);\n          })\n          .catch((err) => console.log(err.message));\n    },\n    setDefaultValues() {\n      if (this.studentsnumbers) {\n        this.studentsnumbers = this.course.studentsnumbers;\n      }\n      if (this.groupsnumbers) {\n        this.groupsnumbers = this.course.groupsnumbers;\n      }\n      if (this.description) {\n        this.description = this.course.description;\n      }\n    },\n    updateCourse() {\n      this.setDefaultValues();\n      const id = this.$route.params.id;\n      console.log(this.studentsnumbers, this.groupsnumbers, this.description)\n      fetch(`http://localhost:3000/api/courses/${id}`, {\n        method: \"PUT\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify({\n\n          studentsnumbers: this.studentsnumbers,\n          groupsnumbers: this.groupsnumbers,\n          description: this.description,\n        }),\n      })\n        .then((response) => response.json())\n        .then((data) => {\n          console.log(data);\n          this.$router.push({ name: \"Courses\" });\n        })\n        .catch((err) => console.log(err.message));\n    },\n  },\n  mounted() {\n    this.fetchRecords();\n  }\n};\n</script>\n\n<style scoped>\n.form {\n  max-width: 420px;\n  margin: 30px auto;\n  background: rgb(167, 154, 154);\n  text-align: left;\n  padding: 40px;\n  border-radius: 10px;\n}\nh3 {\n  text-align: center;\n  color: rgb(8, 110, 110);\n}\nlabel {\n  color: rgb(8, 110, 110);\n  display: inline-block;\n  margin: 25px 0 15px;\n  font-size: 0.8em;\n  text-transform: uppercase;\n  letter-spacing: 1px;\n  font-weight: bold;\n}\ninput {\n  display: block;\n  padding: 10px 6px;\n  width: 100%;\n  box-sizing: border-box;\n  border: none;\n  border-bottom: 1px solid white;\n  color: black;\n  text-align: center;\n}\nbutton {\n  background: rgb(8, 110, 110);\n  border: 0;\n  padding: 10px 20px;\n  margin-top: 20px;\n  color: white;\n  border-radius: 20px;\n  align-items: center;\n  text-align: center;\n}\n\n::placeholder {\n  text-align: center;\n}\n</style>"],"mappings":";AAgBA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,EAAE;MACnBC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,MAAMC,EAAC,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,EAAE;MAChCG,KAAK,CAAE,qCAAoCH,EAAG,EAAC,EAC1CI,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEX,IAAI,IAAK;QACd,IAAI,CAACC,MAAK,GAAID,IAAI;QAClBc,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,MAAM,CAAC;MAC1B,CAAC,EACAe,KAAK,CAAEC,GAAG,IAAKH,OAAO,CAACC,GAAG,CAACE,GAAG,CAACC,OAAO,CAAC,CAAC;IAC/C,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,IAAI,C